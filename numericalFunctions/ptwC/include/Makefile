SHELL = /bin/sh

sourceDir = ../Src

.PHONY: default clean realclean

default:
	cp $(sourceDir)/nf_utilities.h .
	cp $(sourceDir)/nf_buffer.h .

clean:
	rm -f *.h

realclean: clean
